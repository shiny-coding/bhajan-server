{
	"compilerOptions": {
	  "target": "ES6",
	  "module": "CommonJS",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "outDir": "./dist",
	  "rootDir": "./src",
	  "plugins": [
	    { "transform": "ts-transformer-keys/transformer" }
	  ]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}